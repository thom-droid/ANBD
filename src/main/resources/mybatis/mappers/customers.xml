<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="customers">
	<insert id="insertCustomers" parameterType="java.util.Map">
		INSERT INTO customers (no, id, name)
		Values
		(customers_seq.nextval, #{id},#{name})
	</insert>


</mapper>

