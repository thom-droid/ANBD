<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="rcpsSaved">

<select parameterType="int" id="count" resultType="int">
SELECT count(*)
FROM rcps_saved
WHERE recipe_no=#{no}
</select>

</mapper>

